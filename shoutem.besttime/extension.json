{
  "name": "besttime",
  "title": "BestTime",
  "version": "5.0.1",
  "platform": ">=9",
  "description": "Check the busyness of an establishment using BestTime.",
  "releaseNotes": "* Added support for platform 9.0.0",
  "nativeChanges": {
    "lastVersion": "5.0.1"
  },
  "dependencies": [
    "shoutem.application",
    "shoutem.cms",
    "shoutem.i18n",
    "shoutem.navigation"
  ],
  "icon": "server/assets/add-besttime-image.png",
  "screens": [
    {
      "name": "SearchScreen"
    },
    {
      "name": "ForecastScreen"
    },
    {
      "name": "MapScreen"
    }
  ],
  "shortcuts": [
    {
      "name": "BestTimeSearch",
      "title": "BestTime Search",
      "description": "Search for establishments to check their busyness using BestTime.",
      "screen": "@.SearchScreen",
      "navigatesTo": ["@.ForecastScreen"],
      "adminPages": [
        {
          "page": "@.LocationBiasingPage",
          "title": "Location Biasing"
        }
      ],
      "settings": {
        "radius": "50000",
        "useLocationBiasing": false
      }
    }
  ],
  "pages": [
    {
      "name": "ApiKeysPage",
      "type": "react-page"
    },
    {
      "name": "LocationBiasingPage",
      "type": "react-page"
    }
  ],
  "settingsPages": [
    {
      "page": "@.ApiKeysPage",
      "title": "API keys"
    }
  ]
}
