{
  "name": "besttime",
  "title": "BestTime",
  "version": "5.0.0",
  "platform": ">=6",
  "description": "Check the busyness of an establishment using BestTime.",
  "releaseNotes": "*  Added support for new extension scoped theming",
  "icon": "server/assets/add-besttime-image.png",
  "screens": [
    {
      "name": "SearchScreen"
    },
    {
      "name": "ForecastScreen"
    },
    {
      "name": "MapScreen"
    }
  ],
  "shortcuts": [
