      "navigatesTo": ["@.ForecastScreen"],
      "adminPages": [
        {
          "page": "@.LocationBiasingPage",
          "title": "Location Biasing"
        }
      ],
      "settings": {
        "radius": "50000",
        "useLocationBiasing": false
      }
    }
  ],
  "pages": [
    {
      "name": "ApiKeysPage",
      "type": "react-page"
    },
    {
      "name": "LocationBiasingPage",
      "type": "react-page"
    }
  ],
  "settingsPages": [
    {
      "page": "@.ApiKeysPage",
      "title": "API keys"
    }
  ]
}
