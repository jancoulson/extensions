{
  "name": "in-app-purchases",
  "version": "4.0.5",
  "platform": "^4 || ^5",
  "title": "In App Purchases",
  "description": "Add Apple/Google Play subscriptions to your app",
  "releaseNotes": "* Fixed ToS & privacy policy links not opening in edge cases\n* Fixed settings pages - previously not being updated on save, in edge cases",
  "icon": "server/assets/iap.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/shoutem/extensions"
  },
  "i18n": {
    "locale": "en",
    "translationPath": "server/translations/en.json"
  },
  "dependencies": [
    "shoutem.application",
    "shoutem.i18n",
    "shoutem.navigation",
    "shoutem.preview",
    "shoutem.redux",
    "shoutem.web-view"
  ],
  "screens": [
    {
      "name": "SubscriptionsScreen"
    }
  ],
  "pages": [
    {
      "name": "IAPSettingsPage",
      "type": "react-page"
    },
    {
      "name": "LockedScreensPage",
      "type": "react-page"
    },
    {
      "name": "PrivacyPolicyPage",
      "type": "react-page"
    },
    {
      "name": "TermsOfServicePage",
      "type": "react-page"
    },
    {
      "name": "SubscriptionScreenPage",
      "type": "react-page"
    }
  ],
  "settingsPages": [
    {
      "page": "@.IAPSettingsPage",
      "title": "General"
    },
    {
      "page": "@.LockedScreensPage",
      "title": "Subscription restrictions"
    },
    {
      "page": "@.PrivacyPolicyPage",
      "title": "Privacy policy"
    },
    {
      "page": "@.TermsOfServicePage",
      "title": "Terms of Service"
    },
    {
      "page": "@.SubscriptionScreenPage",
      "title": "Subscription Screen"
    }
  ],
  "settings": {
    "iOSProductId": "",
    "androidProductId": "",
    "iapHubAppId": "",
    "iapHubApiKey": "",
    "iapHubEnvironment": "production",
    "subscriptionRequired": false,
    "termsOfServiceUrl": "",
    "privacyPolicyUrl": "",
    "subscriptionScreenTitle": "",
    "subscriptionScreenDescription": "",
    "subscriptionScreenImageUrl": "",
    "confirmationMessageTrial": "",
    "confirmationMessageRegular": "",
    "allScreensProtected": false
  }
}
