{
  "name": "salesforce",
  "title": "Salesforce MC",
  "version": "1.0.5",
  "description": "Salesforce Marketing Cloud",
  "platform": ">=6",
  "nativeChanges": {
    "lastVersion": "1.0.5",
    "settingsKeys": ["appId", "accessToken", "appEndpoint", "fcmSenderId"]
  },
  "releaseNotes": "* Fixed the Android crash if extension wasn't properly configured in settings page",
  "dependencies": [
    "shoutem.application",
    "shoutem.auth",
    "shoutem.firebase",
    "shoutem.preview"
  ],
  "i18n": {
    "locale": "en",
    "translationPath": "server/translations/en.json"
  },
  "settings": {
    "clientId": null,
    "authBaseUri": null,
    "restBaseUri": null,
    "salesforceAuthorized": false,
    "appId": null,
    "accessToken": null,
    "appEndpoint": null,
    "fcmSenderId": null
  },
  "pages": [
    {
      "name": "SalesforceSettingsPage",
      "type": "react-page"
    }
  ],
  "settingsPages": [
    {
      "page": "@.SalesforceSettingsPage",
      "title": "Settings"
    }
  ],
  "cloud": [
    {
      "type": "api",
      "script": "npm run migrate:docker && npm run start:server:docker"
    }
  ]
}
