{
  "name": "cms",
  "version": "4.2.1",
  "platform": "^5",
  "type": "system",
  "title": "CMS",
  "description": "Shoutem CMS extension provides the integration with the Shoutem CMS. Extensions using CMS extension are able to fetch data from CMS and use Shoutem CMS editor.",
  "releaseNotes": "* Added option for reordering categories in settings page\n* Removed animated loading placeholder when loading more list items in-app",
  "icon": "server/assets/add-cms-image.png",
  "categories": ["content"],
  "dependencies": [
    "shoutem.application",
    "shoutem.layouts",
    "shoutem.i18n",
    "shoutem.navigation",
    "shoutem.permission",
    "shoutem.redux"
  ],
  "i18n": {
    "locale": "en",
    "translationPath": "server/translations/en.json"
  },
  "settingsPages": [
    {
      "page": "@.CmsSettingsPage",
      "title": "Settings"
    }
  ],
  "pages": [
    {
      "name": "CmsPage",
      "type": "native-component",
      "path": "dummy.js"
    },
    {
      "name": "CmsSettingsPage",
      "type": "native-component",
      "path": "dummy.js"
    }
  ],
  "settings": {
    "apiEndpoint": "http://api.beta.shoutem.com"
  }
}
